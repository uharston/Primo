    <style>
      body,h1 .underline {
        font-family: "Raleway", sans-serif;
        color: black;
        font-weight: bold;
      }
      html, body {
        height: 100%;
        width: 100%;
        margin:0;
      }
      body {
        /* height: 100vh; */
      }

      .content-wrapper {
        height: 80vh;
        display: flex;
      }

      .form-card {
        margin:auto; 
        width: 500px;
        padding-bottom: 15px;
      }

      .footer {
        align-self: end;
     
      }

      .alt-footer {
        text-align: center;
        padding-bottom: 10px;
      }

      #content-wrap {
      padding-bottom: 2.5rem;    /* Footer height */
    }


      .bgimg {
        background-image: url('/images/skater_lipslide.jpg');
        height: 94.2%;
        width: 100%;
        background-position: center;
        background-size: cover;
      }

      .background-image-2 {
        background: url('https://miro.medium.com/max/10946/1*apDwuMqNWJU-UF8MzY7RkQ.jpeg'); 
        height: 100%;
        width: 100%;
        background-position: center;
        background-size: cover;
      }
      .background-image-3 {
        background: url('https://windows10spotlight.com/wp-content/uploads/2017/10/50f1b025d7eaa324229046008bde567f.jpg'); 
        height: 100%;
        width: 100%;
        background-position: center;
        background-size: cover;
        margin: auto;
      }

      .background-image-4 {
        background-color: white;
      }

      .background-image-5 {
        background: url('https://i2.wp.com/hypebeast.com/wp-content/blogs.dir/13/files/2019/01/Screen-Shot-2019-01-16-at-11.29.44-AM.png?w=960'); 
        height: 100%;
        width: 100%;
        background-position: center;
        background-size: cover;
        margin: auto;
      }

      .error-message {
        color: white; 
        font-weight: bolder;
      }


      #img-logo {
        width: 700px; 
        height: auto;
      }
      #nav-logo {
        width: 77px;
        height: auto; 
        float: left;
      }

      @media screen and (max-width: 545px) {
        #img-logo {
          width: 300px;
          height: auto;
        }

      }

      /* Add a black background color to the top navigation */
      .topnav {
        background-color: black;
        overflow: hidden;
        margin-bottom: 25px;
      }

      /* Style the links inside the navigation bar */

      .topnav a {
        float: left;
        display: block;
        color: gray;
        text-align: center;
        padding: 6px 16px;
        text-decoration: none;
        font-size: 15px;
      }

      .topnav-right {
        float: right;
      }

      /* Change the color of links on hover */
      .topnav a:hover {
        /* background-color: #ddd; */
        color: white;
      }

      /* Add an active class to highlight the current page */
      .topnav a.active {
        /* background-color: #4CAF50; */
        color: gray;
      }

      /* Hide the link that should open and close the topnav on small screens */
      .topnav .icon {
        display: none;
      }

      /* When the screen is less than 600 pixels wide, hide all links, except for the first one ("Home"). Show the link that contains should open and close the topnav (.icon) */
      @media screen and (max-width: 450px) {
        .topnav a:not(.brand-logo) {display: none;}
        .topnav a.icon {
          float: right;
          display: block;
        }
      }

      /* The "responsive" class is added to the topnav with JavaScript when the user clicks on the icon. This class makes the topnav look good on small screens (display the links vertically instead of horizontally) */
      @media screen and (max-width: 375px) {
        .topnav.responsive {position: relative;}
        .topnav.responsive a.icon {
          position: absolute;
          right: 0;
          top: 0;
        }
        .topnav.responsive a {
          float: none;
          display: block;
          text-align: center;
        }
          .topnav.responsive {
          float: none;
          display: block;
          text-align: left;
        }

        .topnav-right {
          float: none;
          padding: 14px 16px;
        }
        .icon {
          color: white;
        }
      }
        </style>
  <body>

    <%=yield%>

    <script>
    function myFunction() {
      var x = document.getElementById("myTopnav");
      if (x.className === "topnav") {
        x.className += " responsive";
      } else {
        x.className = "topnav";
      }
    }
</script>
    
